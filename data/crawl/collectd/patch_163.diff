+	passthru($rrd_cmd, $rt);
+	if ($rt != 0)
+		return error500($graph_identifier, "RRD failed to generate the graph: ".$rt);
+	return $rt;
+} else {
+	return error500($graph_identifier, "Failed to tell RRD how to generate the graph");
+}
