-	$(MAKE) -C ./latex pdf
-	ln -s ./latex/refman.ps	 @PACKAGE@.ps
-	ln -s ./latex/refman.pdf @PACKAGE@.pdf
-
-
-clean-local:
-	rm -rf latex
-	rm -rf html man @PACKAGE@.ps @PACKAGE@.pdf
