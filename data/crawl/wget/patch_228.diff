+	 $(MMSTARGETS)
+.ENDIF                              # MAKE_GENERATED_HEADERS
+	set default [-.$(DIR_LIB)]
+	show default
+	@ write sys$output ""
+	$(MMS) /description = $(MMSDESCRIPTION_FILE) $(MMSQUALIFIERS) -
