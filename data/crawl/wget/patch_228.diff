+    char *ret;
+    int sts;
+    FILE *sdc;
+
+    ret = NULL;
+
+    sdc = fopen( "SYS$COMMAND", "r");
+    if (sdc != NULL)
+    {
+        sts = vms_set_term_echo( 0);
+        if (sts == 0)
+        {
+            /* Read password string. */
+            if (sts == 0)
+            {
+                fprintf( stdout, "%s", prompt);
+                sts = fread( pw_buf, 1, PASS_MAX, sdc);
+                if ((sts > 0) && (pw_buf[ sts- 1]) == '\n')
+                {
+                    pw_buf[ --sts] = '\0';
+                }
+                else
+                {
+                    pw_buf[ sts = '\0'];
+                }
+                ret = pw_buf;
+            }
+            sts = vms_set_term_echo( -1);
+        }
+        fclose( sdc);
+    }
