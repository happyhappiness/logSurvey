+function collectd_list_hosts() {
+	global $config;
+
+	$hosts = array();
+	foreach($config['datadirs'] as $datadir)
+		if ($d = @opendir($datadir)) {
+			while (($dent = readdir($d)) !== false)
+				if ($dent != '.' && $dent != '..' && is_dir($datadir.'/'.$dent) && preg_match(REGEXP_HOST, $dent))
+					$hosts[] = $dent;
