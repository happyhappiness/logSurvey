-static const char *
-_nl_expand_alias (name)
-    const char *name;
-{
-  static const char *locale_alias_path = ALIASPATH;
-  struct alias_map *retval;
-  const char *result = NULL;
-  size_t added;
-
-#ifdef _LIBC
-  __libc_lock_lock (lock);
-#endif
