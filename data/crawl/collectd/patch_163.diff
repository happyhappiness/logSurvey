+/**
+ * Alphabetically compare host names, comparing label
+ * from tld to node name
+ */
+function collectd_compare_host($a, $b) {
+	$ea = explode('.', $a);
+	$eb = explode('.', $b);
+	$i = count($ea) - 1;
+	$j = count($eb) - 1;
+	while ($i >= 0 && $j >= 0)
+		if (($r = strcmp($ea[$i--], $eb[$j--])) != 0)
+			return $r;
+	return 0;
+}
