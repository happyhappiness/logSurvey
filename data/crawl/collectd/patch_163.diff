+			if (get_magic_quotes_gpc()) {
+				$ret = array();
+				while (list($k, $v) = each($array[$name]))
+					$ret[stripslashes($k)] = stripslashes($v);
+				return $ret;
+			} else
+				return $array[$name];
+		} else if (is_string($array[$name]) && get_magic_quotes_gpc()) {
+			return stripslashes($array[$name]);
+		} else
+			return $array[$name];
+	} else
+		return $default;
+}
